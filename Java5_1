public  class Java5_1{
  public static void main(String[] args){
    introduceOneself();
  }


  public static void introduceOneself(){
    String name = "犬";
    int age = 15;
    double height = 18.2;
    char zodiac = '牛';
    System.out.println(name + "です");
    System.out.println("年齢は" + age + "歳です");
    System.out.println("干支は" + zodiac + "です");
  }
}
